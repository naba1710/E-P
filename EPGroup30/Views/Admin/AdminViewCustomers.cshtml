@model List<EPGroup30.Areas.Identity.Data.EPGroup30User>

@{
    ViewData["Title"] = "View Customers";
    Layout = "~/Views/Shared/_AdminNavLayout.cshtml";
}

<h1>@ViewData["Title"]</h1>

<table class="table">
    <thead>
        <tr>
            <th>Customer Name</th>
            <th>Phone Number</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var customer in Model)
        {
            <tr>
                <td>@customer.CustomerName</td>
                <td>@customer.PhoneNumber</td>
                <td>@customer.Email</td>
            </tr>
        }
    </tbody>
</table>
