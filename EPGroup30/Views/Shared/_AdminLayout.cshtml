<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Include common CSS and scripts -->
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - EPGroup30 Admin</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    

    <!-- Add specific CSS for the AdminHome page -->
    <link rel="stylesheet" href="~/css/admin-home.css" asp-append-version="true" />

    <!-- Include common scripts -->
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)

          <style >
        body {
            background-color: beige;
        }

         

           
    </style>

</head>
<body>
   
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    
    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - EPGroup30 - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
</body>
</html>
